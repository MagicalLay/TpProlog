creerBinomes(_,[],[]).
creerBinomes(A,[B|Suite],[(A,B)|Suite2]) :- creerBinomes(A,Suite,Suite2). 

combiner([],[]).
combiner([C1|Suite], [B1|Suite2]) :- creerbinomes(C1, Suite, Suite2),
		combiner(Suite, Suite2).
